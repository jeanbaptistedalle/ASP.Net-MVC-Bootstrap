@model TestApp2.ViewsModels.Auth.LoginModel
@{
    ViewBag.Title = "Log In";
}

<h2>Log In</h2>

@Html.ValidationSummary(true)

@using (Html.BeginForm())
{
    @Html.EditorForModel()
    <p>
        <button type="submit">Log In</button>
    </p>
    <a href="@Url.Action("register", "auth")">Register</a>
}